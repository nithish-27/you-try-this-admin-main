       <script>
    $(document).ready(function() {
      $("#imageInput").on('change', function() {
          var previewContainer = $('.preview-container');
          previewContainer.empty(); 
  
          var files = $(this)[0].files;
  
          for (var i = 0; i <files.length; i++) {
              var file = files[i];
              var reader = new FileReader();
  
              reader.onload = function(e) {
                  var image = '<img src="' + e.target.result + '" class="img-fluid mt-3">';
                  previewContainer.append(image);
              };
  
              reader.readAsDataURL(file);
          }
      });
  
      $("#imageForm").submit(function(event) {
          event.preventDefault();
      });
  });
  </script>
  <script>
   document.addEventListener('DOMContentLoaded', function () {
  const imagePreview = document.getElementById('imagePreview');
  const overlayImageInput = document.getElementById('imageUpload');
  const imageInput = document.getElementById('imageInput');
  const images = document.querySelectorAll('.image-container img'); // Select all images

  function updateRequiredAttribute() {
    if (imagePreview.src && imagePreview.src.length > 0) {
      overlayImageInput.removeAttribute('required');
      imageInput.removeAttribute('required');
    } else if (images.length > 0 && images[0].src && images[0].src.length > 0) {
      overlayImageInput.removeAttribute('required');
      imageInput.removeAttribute('required');
    } else {
      overlayImageInput.setAttribute('required', 'required');
      imageInput.setAttribute('required', 'required');
    }
  }

  // Call the function initially
  updateRequiredAttribute();

  // Add an event listener for changes in image sources
  imagePreview.addEventListener('load', updateRequiredAttribute);

  // Add an event listener for changes in images within .image-container
  images.forEach(image => {
    image.addEventListener('load', updateRequiredAttribute);
  });
});

  </script>
